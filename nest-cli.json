{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/assignment/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/assignment/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/assignment",
  "projects": {
    "assignment": {
      "type": "application",
      "root": "apps/assignment",
      "entryFile": "main",
      "sourceRoot": "apps/assignment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/assignment/tsconfig.app.json"
      }
    },
    "doctor": {
      "type": "application",
      "root": "apps/doctor",
      "entryFile": "main",
      "sourceRoot": "apps/doctor/src",
      "compilerOptions": {
        "tsConfigPath": "apps/doctor/tsconfig.app.json"
      }
    },
    "hospital": {
      "type": "application",
      "root": "apps/hospital",
      "entryFile": "main",
      "sourceRoot": "apps/hospital/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hospital/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "patient": {
      "type": "application",
      "root": "apps/patient",
      "entryFile": "main",
      "sourceRoot": "apps/patient/src",
      "compilerOptions": {
        "tsConfigPath": "apps/patient/tsconfig.app.json"
      }
    }
  }
}